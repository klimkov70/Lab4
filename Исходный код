#include <iostream>
#include <string>

using namespace std;

string toWords(int number) {
    string ones[] = {"", "один", "два", "три", "четыре", "пять", "шесть", "семь", "восемь", "девять", "десять",
                          "одиннадцать", "двенадцать", "тринадцать", "четырнадцать", "пятнадцать", "шестнадцать",
                          "семнадцать", "восемнадцать", "девятнадцать"};
    string tens[] = {"", "", "двадцать", "тридцать", "сорок", "пятьдесят", "шестьдесят", "семьдесят", "восемьдесят",
                          "девяносто"};
    string hundreds[] = {"", "сто", "двести", "триста", "четыреста", "пятьсот", "шестьсот", "семьсот", "восемьсот",
                              "девятьсот"};

    if (number == 0) {
        return "ноль";
    }

    string words;

    int thousands = number / 1000;
    if (thousands > 0) {
        words += hundreds[thousands] + " ";
        number %= 1000;
    }

    int hundredsPart = number / 100;
    if (hundredsPart > 0) {
        words += hundreds[hundredsPart] + " ";
        number %= 100;
    }

    if (number >= 20) {
        int tensPart = number / 10;
        words += tens[tensPart] + " ";
        number %= 10;
    }

    if (number > 0) {
        words += ones[number] + " ";
    }

    return words;
}

int main() {
    int number;
    cout << "Введите число от 1 до 100000: ";
    cin >> number;

    if (number < 1 || number > 100000) {
        cout << "Неправильный ввод." << endl;
        return 0;
    }

    string words = toWords(number);
    cout << words << "руб";

    int ones = number % 10;
    int tens = (number / 10) % 10;

    if (tens != 1) {
        if (ones == 1) {
            cout << "ль";
        } else if (ones >= 2 && ones <= 4) {
            cout << "ля";
        } else {
            cout << "лей";
        }
    }

    cout << endl;

    return 0;
}
